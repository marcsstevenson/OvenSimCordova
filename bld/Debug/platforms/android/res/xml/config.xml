<?xml version='1.0' encoding='utf-8'?>
<widget id="Moffat.Oven.Simulator" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <name>Moffat Simulator E32D4</name>
    <description>This is a simulation of the Moffat Oven E32D4</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Marc Stevenson</author>
